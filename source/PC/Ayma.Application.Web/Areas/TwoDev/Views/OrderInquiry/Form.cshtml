@{
    ViewBag.Title = "订单查询";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">机场名称</div>
        <input id="F_AirfieldName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">航站楼</div>
        <input id="F_AirfieldFloor" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">航空公司</div>
        <input id="F_FlightCompany" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">航班号</div>
        <input id="F_FlightNumber" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单日期</div>
        <input id="F_OrderDate" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单号</div>
        <input id="F_OrderNo" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">客户姓名</div>
        <input id="F_CustomerName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">联系电话</div>
        <input id="F_CustomerPhone" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">客户地址</div>
        <input id="F_CustomerAddress" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">客户备注</div>
        <input id="F_CustomerRemarks" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单创建类型</div>
        <input id="F_CreateStype" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单状态</div>
        <input id="F_State" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单类型</div>
        <input id="F_Stype" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单创建日期</div>
        <input id="F_CreateTime" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="T_OrderHead" >
        <div class="am-form-item-title">订单创建人</div>
        <input id="F_CreateUserName" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/TwoDev/Views/OrderInquiry/Form.js")
