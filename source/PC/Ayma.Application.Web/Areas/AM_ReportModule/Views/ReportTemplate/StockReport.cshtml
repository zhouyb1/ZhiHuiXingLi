
@{
    ViewBag.Title = "仓库报表";
    Layout = "~/Views/Shared/_ReportTemplate.cshtml";
}
<div class="am-layout ui-report">
    <div class="am-layout-center">
        <div class="am-layout-wrap am-layout-wrap-notitle">
            <div class="am-layout-tool">
                <div class="am-layout-tool-left">
                    <div class="am-layout-tool-item">
                        <input id="txt_Keyword" type="text" class="form-control" placeholder="请输入要查询关键字" />
                    </div>
                    <div class="am-layout-tool-item">
                        <a id="btn_Search" class="btn btn-primary btn-sm"><i class="fa fa-search"></i>&nbsp;查询</a>
                    </div>
                </div>
                <div class="am-layout-tool-right">
                    <div class="btn-group btn-group-sm" ayma-authorize="no">
                        <a id="am-replace" class="btn btn-default" ><i class="fa fa-refresh"></i>&nbsp;刷新</a>
                        <a id="am-print" class="btn btn-default"><i class="fa fa-print"></i>&nbsp;打印</a>
                        <a id="am-export" class="btn btn-default" ><i class="fa fa-sign-out"></i>&nbsp;导出</a>
                    </div>
                </div>
            </div>
            <div class="am-layout-grid">
                <div class="grid-title">商品收发明细表</div>
                <div class="grid-subtitle">日期: 2010-03-05 至 2016-03-28</div>
                <div class="am-layout-body" id="girdtable"></div>
            </div>
        </div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AM_ReportModule/Views/ReportTemplate/StockReport.js")
