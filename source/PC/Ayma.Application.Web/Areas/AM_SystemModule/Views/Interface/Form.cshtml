@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
@Html.AppendCssFile("/Areas/AM_SystemModule/Views/Interface/Form.css")
<div class="am-form-layout">
    <div class="am-form-layout-header" id="form">
        <div class="col-xs-12 am-form-item">
            <div class="am-form-item-title">接口名称<font face="宋体">*</font></div>
            <input id="F_Name" type="text" class="form-control am-input-underline" placeholder="接口名称" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-12 am-form-item">
            <div class="am-form-item-title">接口地址<font face="宋体">*</font></div>
            <input id="F_Address" type="text" class="form-control am-input-underline" placeholder="接口地址" isvalid="yes" checkexpession="NotNull" />
        </div>
    </div>
    <div class="am-form-layout-body">
        <div class="am-form-layout-body-title">
            <div class="am-form-jfgrid-btns">
                <a id="am_add_field" class="btn btn-success"><i class="fa fa-plus"></i>&nbsp;添加</a>
                <a id="am_edit_field" class="btn btn-info"><i class="fa fa-pencil-square-o"></i>&nbsp;修改</a>
                <a id="am_delete_field" class="btn btn-danger"><i class="fa fa-trash-o"></i>&nbsp;移除</a>
            </div>
            <span>数据权限过滤字段维护</span>
        </div>
        <div id="girdtable"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AM_SystemModule/Views/Interface/Form.js")
