<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta name=”renderer” content=”webkit|ie-comp|ie-stand” />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <title>@ViewBag.Title</title>
    <link href="~/Content/font/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap/bootstrap.min.css" rel="stylesheet" />

    <link href="~/Content/jquery/plugin/scrollbar/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <link href="~/Content/jquery/plugin/jfGrid/jfgrid.css" rel="stylesheet" />

    @Html.AppendCssFile(
    "/Views/AM_Content/style/am-common.css",
    "/Views/AM_Content/style/am-iframe-form.css",
    "/Views/AM_Content/plugin/wizard/wizard.css",
    "/Views/AM_Content/plugin/tree/am-tree.css",
    "/Views/AM_Content/plugin/select/am-select.css",
    "/Views/AM_Content/plugin/formselect/am-formselect.css",
    "/Views/AM_Content/plugin/inputsearch/am-isearch.css",
     "/Views/AM_Content/plugin/uploader/am-uploader.css"
    )
    <script src="~/Content/jquery/jquery-1.10.2.min.js"></script>
    <script src="~/Content/jquery/plugin/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Content/jquery/plugin/jfGrid/jfgrid.js"></script>
    <script src="~/Content/jquery/plugin/jqprint/jquery.jqprint-0.3.js"></script>
    <script src="~/Content/datepicker/WdatePicker.js"></script>
    <!--[if lt IE 9]>
      <script src="~/Content/bootstrap/html5shiv.min.js"></script>
      <script src="~/Content/bootstrap/respond.min.js"></script>
    <![endif]-->
    <script>
    function request(d) { for (var c = location.search.slice(1).split("&"), a = 0; a < c.length; a++) { var b = c[a].split("="); if (b[0] == d) if ("undefined" == unescape(b[1])) break; else return unescape(b[1]) } return "" };
    </script>
   
</head>
<body class="@Ayma.Util.Net.Browser @Ayma.Util.WebHelper.GetUITheme()">
    <div id="am_form_bg" class="am-form-bg"></div>
    @RenderBody()


    @Html.AppendJsFile(
    "/Views/AM_Content/script/am-validator.js",
    "/Views/AM_Content/script/am-form.js",
    "/Views/AM_Content/plugin/wizard/wizard.js",
    "/Views/AM_Content/plugin/tree/am-tree.js",
    "/Views/AM_Content/plugin/select/am-select.js",
    "/Views/AM_Content/plugin/formselect/am-formselect.js",
    "/Views/AM_Content/plugin/inputsearch/am-isearch.js",
    "/Views/AM_Content/plugin/uploader/am-uploader.js"
    )
    <script>
        $.amToken = $('@Html.AntiForgeryToken()').val();
        $(function () {
            if (!!window.bootstrap) {
                bootstrap(jQuery, top.ayma);
            }
            $('#am_form_bg').fadeOut();
        });
    </script>
</body>
</html>
